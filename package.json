{
  "name": "react-ui-demo",
  "version": "0.0.1",
  "main": "server.js",
  "description": {
    "tips": [
      "preinstall:npm install --save @types/react @types/react-dom @types/react-router",
      "test: node test/all.js",
      "lint: eslint yourJsFiles.js",
      "precommit: npm run test && npm run lint",
      "prepush: npm test",
      "publishpatch: npm run deploy; git add --all; git commit -m 'c'; npm version patch; git push origin master:master; npm publish; npm publish --registry='https://registry.npmjs.org'; cnpm sync react-mgm; npm version;"
    ]
  },
  "scripts": {
    "start": "nodemon server.js",
    "start-mongo": "mongod --dbpath /Users/huyong/mongodb/data/db1",
    "stop-mongo": "mongo && use admin && db.shutdownServer()",
    "start-server": "nodemon start-server.js",
    "start-man": "cd database && nodemon server.js",
    "jsdemo": "node jsdemo",
    "build": "webpack --config webpack.production.js --progress && npm run clean && mv _dist y_dist",
    "clean": "rimraf y_dist",
    "pkgdemo": "cd builder && NODE_ENV=production webpack --config webpack.build.js",
    "publish": "cd builder/ybuild && npm version patch && npm publish",
    "imgmin": "gulp",
    "transJs": "cd tools && rm -rf ./*.js && babel ../server.js -o server.es5.js && uglifyjs server.es5.js -c -m -e -o server.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/react-ui-demo"
