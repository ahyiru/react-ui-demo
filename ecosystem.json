{
  "apps": [{
    "name": "react-ui-demo",
    "script": "index.js",
    "log_date_format": "YYYY-MM-DD HH:mm:ss"
  }],
  "deploy" : {
    "stage" : {
      "user" : "deploy",
      "host" : "https://github.com",
      "ref"  : "origin/master",
      "repo" : "git@github.com:ahyiru/react-ui-demo.git",
      "path" : "~/react-ui-demo",
      "post-deploy" : "npm i && npm run build"
    },
    "productoin" : {
      "user" : "deploy",
      "host" : "https://github.com",
      "ref"  : "origin/master",
      "repo" : "git@github.com:ahyiru/react-ui-demo.git",
      "path" : "~/react-ui-demo",
      "post-deploy" : "npm i && npm run build"
    }
  }
}
